{
	"extends": "eslint:recommended",
	"env": {
			"commonjs": true,
			"es6": true,
			"node": true,
			"jest": true
	},
	"rules": {
			"linebreak-style": [
					"error",
					"unix"
			],
			"quotes": [
					"error",
					"single"
			],
			"semi": [
					"error",
					"always"
			],
			"no-console": "off"
	}
}